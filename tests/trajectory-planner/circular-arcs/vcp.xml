            <halpin>"nyacc"</halpin>
            <font>("Helvetica",24)</font>
            <format>"+4.4f"</format>
        </number>
        <label>
            <text>"-Vel"</text>
        </label>
        <number>
            <halpin>"nyvel"</halpin>
            <font>("Helvetica",24)</font>
